{
	"manifest_version": 3,
	"name": "aad",
	"version": "1.1",
	"permissions": ["downloads"],
	"host_permissions": ["https://arca.live/*", "https://*.namu.la/*"],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "dist/sw.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["https://arca.live/*"],
			"js": ["dist/content.js"]
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["*://arca.live/*"],
			"resources": ["style.min.css"]
		}
	]
}
